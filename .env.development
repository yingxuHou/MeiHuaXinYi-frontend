# 开发环境配置

# 应用标题
VITE_APP_TITLE=梅花心易

# API基础URL
VITE_API_BASE_URL=http://localhost:8080/api

# 应用版本
VITE_APP_VERSION=2.0.0

# 开发模式
VITE_DEV_MODE=true

# 是否启用模拟数据
VITE_ENABLE_MOCK=false

# 是否启用调试模式
VITE_DEBUG=true

# 是否启用性能监控
VITE_ENABLE_PERFORMANCE=true

# 上传文件大小限制（MB）
VITE_UPLOAD_SIZE_LIMIT=10

# 分页默认大小
VITE_PAGE_SIZE=10

# Token过期时间（小时）
VITE_TOKEN_EXPIRE_HOURS=24

# 刷新Token过期时间（天）
VITE_REFRESH_TOKEN_EXPIRE_DAYS=7

# 是否启用PWA
VITE_ENABLE_PWA=true

# 是否启用服务工作者
VITE_ENABLE_SERVICE_WORKER=true

# 分析工具配置
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=

# 错误监控配置
VITE_ENABLE_ERROR_TRACKING=false
VITE_ERROR_TRACKING_DSN=

# 第三方服务配置
VITE_WECHAT_APP_ID=
VITE_ALIPAY_APP_ID=

# CDN配置
VITE_CDN_URL=

# 静态资源URL
VITE_STATIC_URL=

# WebSocket URL
VITE_WS_URL=ws://localhost:8080

# 地图服务配置
VITE_MAP_KEY=

# 推送服务配置
VITE_PUSH_PUBLIC_KEY=
